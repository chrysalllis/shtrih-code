<Window x:Class="QQwert1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QQwert1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button x:Name="btGenerator" Click="btGenerator_Click" HorizontalAlignment="Center" Margin="10" Width="200"> сгенерировать штрих-код</Button>
            
            <Canvas x:Name="canvasCode" Width="300" Height="100"  HorizontalAlignment="Center" Margin="10" ></Canvas>
            
            <Label x:Name="labelCode" Margin="100" FontSize="20" HorizontalContentAlignment="Center"></Label>
        </StackPanel>
    </Grid>
</Window>
